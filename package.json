{
  "name": "LevelPad",
  "version": "0.0.1",
  "description": "Webapp zur Unterstützung einer Bewertung nach dem Niveaustufenmodell",
  "private": true,
  "scripts": {
    "postinstall": "node ./node_modules/.bin/bower install",
    "run": "node server.js",
    "run-auto": "node ./node_modules/.bin/nodemon --delay 2.5 --ignore public server.js",
    "test": "NODE_ENV=test node ./node_modules/.bin/mocha --recursive --reporter spec app/tests",
    "test-cov": "NODE_ENV=test node ./node_modules/.bin/istanbul cover --report lcovonly ./node_modules/.bin/_mocha -- --recursive app/tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/fh-koeln/LevelPad"
  },
  "keywords": [
    "FH",
    "Köln"
  ],
  "author": "fh-koeln",
  "bugs": {
    "url": "https://github.com/fh-koeln/LevelPad/issues"
  },
  "homepage": "https://github.com/fh-koeln/LevelPad",
  "engines": {
    "node": "0.10.x"
  },
  "dependencies": {
    "acl": "0.4.2",
    "async": "~0.9.0",
    "body-parser": "~1.5.0",
    "compression": "~1.0.9",
    "connect-mongo": "~0.4.1",
    "connect-timeout": "~1.2.0",
    "cookie-parser": "~1.3.2",
	"debug": "*",
    "errorhandler": "~1.1.1",
    "express": "~4.6.1",
    "express-session": "~1.6.5",
    "helmet": "0.4.0",
    "imap": "~0.8.12",
    "mongoose": "~3.8.13",
    "morgan": "~1.2.0",
    "passport": "~0.2.0",
    "passport-local": "~1.0.0",
    "path-to-regexp": "~0.2.3"
  },
  "devDependencies": {
    "bower": "*",
    "nodemon": "*",
    "connect-livereload": "*",
    "mocha": "*",
    "mocha-lcov-reporter": "*",
    "istanbul": "*",
    "supertest": "~0.13.0",
    "should": "~4.0.4"
  }
}
